google-maps
  width: 100vw
  height: 100vh
  position: relative
  display: block


  .zoom-in
    background-image: url("/assets/images/plus.png")
    background-size: cover
    width: 70px
    height: 70px
    cursor: pointer

  .zoom-out
    background-image: url("/assets/images/minus.png")
    background-size: cover
    background-position-x: -3px
    width: 70px
    height: 70px
    cursor: pointer


