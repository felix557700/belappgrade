@font-face
  font-family: 'icomoon'
  src: url('assets/fonts/icomoon.ttf?rr96jp') format('truetype'),
      url('assets/fonts/icomoon.woff?rr96jp') format('woff'),
      url('assets/fonts/icomoon.svg?rr96jp#icomoon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"],
[class*=" icon-"]
  font-family: 'icomoon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.icon
  font-size: 100px
  color: #fff
  cursor: pointer

.icon-mask-simple:before
  content: "\e900"

.icon-bulb:before
  content: "\e901"

.icon-mask:before
  content: "\e902"

.icon:hover
.orange
  color: orange

